    "@types/styled-components": "^5.1.26",
    "rimraf": "^6.0.1",
    "styled-components": "^6.1.13",
    "typescript": "^5.8.3"
  },
  "peerDependencies": {
    "@mui/material": "^6.1.3",
    "@mui/styled-engine-sc": "^6.1.3",
    "react": "^19.0.0",
